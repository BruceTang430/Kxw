        <!-- 第12行 --> 
        <TableRow android:id="@+id/tableRow16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp">
            <TextView/>
            <TextView android:text="司机手机号:"
                android:id="@+id/txt_driver_tel"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#4f94cd"
                
            /> 
            <EditText android:id="@+id/edit_driver_tel"
                android:textSize="15dp"
                android:layout_width="110dip"
                android:layout_height="wrap_content"
                android:singleLine="true"
                android:inputType="number"/>
            </TableRow>            
        <!-- 第12行 --> 
        <TableRow android:id="@+id/tableRow173"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp">
            <TextView/>
            <TextView android:text="驾驶证号:"
                android:id="@+id/txt_driver_license"
