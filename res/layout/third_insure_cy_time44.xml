            <TextView />
            <TextView android:text="收货单位:"
                android:id="@+id/txt_shorg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#4f94cd"
            />
            <EditText android:id="@+id/edit_shorg"
                android:textSize="15dp"
                android:layout_width="200dip"
                android:layout_height="wrap_content"
                android:singleLine="true"/> 
            <TextView />
        </TableRow> 
        <!-- 第22行 -->
        <TableRow android:id="@+id/tableRow222222222"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dip">
            <TextView />               
            <TextView android:text="收货人手机:"
                android:id="@+id/txt_sssshorg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#4f94cd"
